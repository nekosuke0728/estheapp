<div class="panel-group" id="accordion">

  <div class="panel panel-danger">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne">開発用一時メニュー</a>
      </h4>
    </div>
    <div id="collapseOne" class="panel-collapse collapse in">
      <div class="panel-body">
        <%= link_to "ブランド", brands_path %>
      </div>
      <div class="panel-body">
        <%= link_to "カテゴリ", staff_categories_path %>
      </div>
      <div class="panel-body">
        <%= link_to "アイテムタイプ", item_types_path %>
      </div>
      <div class="panel-body">
        <%= link_to "商品", products_path %>
      </div>
      <div class="panel-body">
        <%= link_to "在庫", stocks_path %>
      </div>
      <div class="panel-body">
        <%= link_to "ニュース分類", news_types_path %>
      </div>
      <div class="panel-body">
        <%= link_to "ニュース記事", news_index_path %>
      </div>
      <div class="panel-body">
        <%= link_to "エステメニュー", esthe_menus_path %>
      </div>
      <div class="panel-body">
        <%= link_to "予約", reservations_path %>
      </div>
      <div class="panel-body">
        <%= link_to "肌のお悩みタイプ", skin_trouble_types_path %>
      </div>
      <div class="panel-body">
        <%= link_to "支払方法", payments_path %>
      </div>
    </div>
  </div>

  <div class="panel panel-danger">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-parent="#accordion" href="#collapseTwo">予約</a>
      </h4>
    </div>
    <div id="collapseOne" class="panel-collapse collapse">
      <div class="panel-body">
        <%= link_to "予約一覧", reservations_path %>
      </div>
      <div class="panel-body">
        <%= link_to "エステメニュー", esthe_menus_path %>
      </div>
    </div>
  </div>

  <div class="panel panel-danger">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-parent="#accordion" href="#collapseThree">その他</a>
      </h4>
    </div>
    <div id="collapseThree" class="panel-collapse collapse">
        <div class="panel-body">
          <%= link_to "予約", reservations_path %>
        </div>
    </div>
  </div>

  <div class="panel panel-danger">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-parent="#accordion" href="#collapseFour">アカウント</a>
      </h4>
    </div>
    <div id="collapseFour" class="panel-collapse collapse">
      <div class="panel-body">
        <%= link_to "ユーザー情報", accounts_users_path %>
      </div>
      <div class="panel-body">
        <%= link_to "スタッフ情報", accounts_staffs_path %>
      </div>
    </div>
  </div>

</div>
