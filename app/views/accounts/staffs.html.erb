<h1>スタッフアカウント一覧</h1>

<table width="100%">
  <tr>
    <th>スタッフID</th>
    <th>名前</th>
    <th>オーナー権限</th>
    <th>メールアドレス</th>
  </tr>

<% @staffs.each do |s| %>
    <tr>
      <td width="5%"><%= s.id %></td>
      <td width="5%"><%= s.name %></td>
      <td width="10%"><%= s.owner %></td>
      <td width="10%"><%= s.email %></td>
    </tr>
  <% end %>
</table>